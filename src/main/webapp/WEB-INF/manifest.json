{
  "manifest_version":1,
  "type":"WebApp",
  "meta":{
    "name":"Damage Chart",
    "version":"1.0.0",
    "minimum-overwolf-version": "0.77.1.0",
    "author":"Developer Name",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"Demo Web app"
  },
  "data": {
    "start_window": "index",
    "windows": {
      "index": {
        "file": "Files/index.html",
        "transparent": false,
        "resizable": true,
        "size": {
          "width": 1000,
          "height": 800
        },
        "min_size": {
          "width": 200,
          "height": 200
        },
        "max_size": {
          "width": 1000,
          "height": 1000
        }
      }
    }
  },
  "externally_connectable":{
    "matches":[
      "http://*"
    ]
  }
}
